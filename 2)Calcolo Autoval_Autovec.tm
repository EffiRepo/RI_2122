    <\textput>
      Calcoliamo l'energia cinetica
    </textput>

    <\input>
      <with|color|red|(<with|math-font-family|rm|%i>18) >
    <|input>
      polarT(m,L1,L2,L3,th1,th2,th3):=block(

      [T,v,w,z,zp,zpc,zp2,z1,z2,z3,j:sqrt(-1),k],

      k:[z[1],z[2],z[3]],

      /*Esprimiamo ogni punto in coordinate polari*/

      z1:toExp(L1,th1), z2:toExp(L2,th1+th2), z3:toExp(L3,th1+th2+th3),

      print(k[1]," = ",z1),print(k[2]," = ",z2),print(k[3]," = ",z3),

      /*Calcoliamo la somma*/

      z:z1+z2+z3,

      /*Definiamo in modo simbolico le velocità angolari*/

      w:[omega[1],omega[2],omega[3]],

      /*Calcolo la derivata composta*/

      zp:diff(z,th1)*w[1]+diff(z,th2)*w[2]+diff(z,th3)*w[3],

      /*Calcolo il coniugato*/

      zpc:subst(j=-j,zp),

      /*Calcolo z * z'*/

      zp2:fullratsimp(gfactor(expand(trigreduce(demoivre(expand(zp*zpc)))))),

      T:1/2*m*zp2,

      return(T)

      )$
    </input>

